{
  "compilerOptions": {
    "types": [],
    "allowSyntheticDefaultImports": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/actions/*": [
        "./src/Actions/*"
      ],
      "@/context/*": [
        "./src/Context/*"
      ],
      "@/hooks/*": [
        "./src/Hooks/*"
      ],
      "@/lib/*": [
        "./src/lib/*"
      ],
      "@/public/*": [
        "./public/*"
      ],
      "@/providers/*": [
        "./src/Providers/*"
      ],
      "@/types/*": [
        "./src/Types/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "next-env.d.ts",
    "*.ts",
    "*.tsx",
    "*.cjs",
    "*.mjs",
    "**/*.cjs",
    "**/*.mjs",
    "**/*.ts",
    "**/*.tsx",
    "**/**/*.tsx",
    ".next/types/**/*.ts",
    "_next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
